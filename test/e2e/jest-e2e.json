{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^@infra/(.*)*": ["src/@core/infra/$1"],
    "^@domain/(.*)*": ["src/@core/domain/$1*"],
    "^@application/(.*)*": ["src/@core/application/$1"],
    "^@presentation/(.*)*": ["src/@core/presentation/$1"]
  }
}
